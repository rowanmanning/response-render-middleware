{
  "name": "@rowanmanning/response-render-middleware",
  "version": "2.0.0",
  "description": "A small convenience utility to generate Express middleware which renders a view",
  "keywords": [
    "express",
    "middleware",
    "render"
  ],
  "author": "Rowan Manning (https://rowanmanning.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowanmanning/response-render-middleware.git"
  },
  "homepage": "https://github.com/rowanmanning/response-render-middleware",
  "bugs": "https://github.com/rowanmanning/response-render-middleware/issues",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "test": "npm run test:coverage",
    "verify": "eslint .",
    "test:unit": "mocha 'test/unit/**/*.test.js'",
    "test:coverage": "nyc npm run test:unit",
    "project:verify": "npx --yes @rowanmanning/validate-project@2 --type git node-library",
    "project:fix": "npx --yes @rowanmanning/validate-project@2 --type git node-library --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^17.0.0",
    "@rowanmanning/eslint-config": "^3.2.1",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "testdouble": "^3.16.4"
  },
  "main": "lib/render.js",
  "nyc": {
    "eager": true,
    "reporter": [
      "html",
      "text"
    ]
  },
  "mocha": {
    "timeout": 10000
  }
}